{
  "name": "act-syntax-highlighter",
  "displayName": "ACT Syntax Highlighter",
  "description": "Syntax Highlighting for Asynchronous Circuit Toolkit language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "act",
        "aliases": [
          "ACT",
          "act"
        ],
        "extensions": [
          ".act"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "act",
        "scopeName": "source.act",
        "path": "./syntaxes/act.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NukeWolf/act-syntax-highlighter"
  }
}
